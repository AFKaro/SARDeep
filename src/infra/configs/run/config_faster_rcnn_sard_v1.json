  "version": 1,
  "model": {
    "name": "faster_rcnn",
    "version": "default",
    "optimizer": {
      "lr": 0.01,
      "momentum": 0.9,
      "interval": 5
    },
    "runner": {
      "max_epochs": 50
    }
  },
  "dataset": {
    "name": "sard",
    "use": "train|val|test"
  },
  "fine_tune": {
    "name": "faster_rcnn_r50_fpn_1x_coco",
    "load_from": "infra/checkpoints/faster_rcnn_r50_fpn_1x_coco.pth"
  }
}